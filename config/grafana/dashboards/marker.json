  "links": [],
  "panels": [
    {
      "type": "stat",
      "title": "Entries marked (total)",
      "id": 1,
      "datasource": null,
      "targets": [
        {
          "expr": "sum(des_marker_entries_marked_total)",
          "legendFormat": "marked",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Batch duration p95 (s)",
      "id": 2,
      "datasource": null,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(des_marker_batch_duration_seconds_bucket[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Errors by type",
      "id": 3,
      "datasource": null,
      "targets": [
        {
          "expr": "sum by (error_type) (rate(des_marker_errors_total[5m]))",
          "legendFormat": "{{error_type}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Retry attempts",
      "id": 4,
      "datasource": null,
      "targets": [
        {
          "expr": "sum by (attempt) (rate(des_marker_retries_total[5m]))",
          "legendFormat": "attempt {{attempt}}",
          "refId": "A"
        }
      ]
    }
  ],
